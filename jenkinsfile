pipeline {
    agent any
    environment{
            PATH = "/opt/maven3/bin:$PATH"
    }
    stages {
        stage('clone') {
            steps {
                git branch: 'main', url: 'https://github.com/Rolex960/jenks_CICD'
            }
        }
        stage('Maven Build') {
            steps {
                sh "mvn clean package"
            }
        }
    }
}
